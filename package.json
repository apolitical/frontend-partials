{
  "name": "@apolitical/frontend-partials",
  "version": "0.6.2",
  "description": "Frontend Partials for use in non react applications",
  "engines": {
    "node": "^10.11.0",
    "npm": "^6.4.1"
  },
  "scripts": {
    "coverage": "codecov -f coverage/lcov.txt",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "dev": "concurrently --names 'SRVR,TEST,LINT,FLOW'  -c 'black.bgCyan.bold,bgBlue.bold,bgMagenta.bold,black.bgGreen.bold' 'npm run start' 'npm run start:test' 'npm run start:lint' 'npm run start:flow'",
    "flow": "flow",
    "lint": "eslint --ext .jsx --ext .js src/",
    "quality": "concurrently --names 'TEST,LINT,FLOW' -c 'bgBlue.bold,bgMagenta.bold,black.bgGreen.bold' 'npm run test' 'npm run lint' 'npm run flow'",
    "start": "NODE_ENV=development webpack-dev-server -d --open --config webpack.config.js",
    "start:flow": "nodemon --exec 'npm run flow || true'",
    "start:lint": "nodemon --exec 'npm run lint || true'",
    "start:test": "nodemon --exec 'npm run test || true'",
    "test": "jest --coverage",
    "audit": "npm-audit-ci -h",
    "preversion": "npm run lint && npm run flow",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apolitical/frontend-partials.git"
  },
  "author": "Daniel Mason <daniel.mason@apolitical.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apolitical/frontend-partials/issues"
  },
  "files": [
    "dist/**/*",
    "CHANGELOG.md",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://github.com/apolitical/frontend-partials#readme",
  "dependencies": {
    "@apolitical/styleguide": "^3.1.6",
    "core-decorators": "^0.20.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.0.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "codecov": "^3.1.0",
    "concurrently": "^4.1.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flow-header": "^0.2.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "flow-bin": "^0.93.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "npm-audit-ci": "^1.1.0",
    "nyc": "^13.1.0",
    "puppeteer": "^1.11.0",
    "regenerator-runtime": "^0.13.1",
    "terser-webpack-plugin": "^1.2.1",
    "webpack": "^4.28.3",
    "webpack-cdn-plugin": "^2.3.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
