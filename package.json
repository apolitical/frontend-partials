{
  "name": "profile",
  "private": true,
  "version": "0.0.0",
  "description": "Profile browser frontend application for Apolitical",
  "main": "index.js",
  "scripts": {
    "coverage": "codecov -f coverage/lcov.txt",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "dev": "concurrently --names 'SRVR,TEST,LINT,FLOW'  -c 'black.bgCyan.bold,bgBlue.bold,bgMagenta.bold,black.bgGreen.bold' 'npm run start' 'npm run start:test' 'npm run start:lint' 'npm run start:flow'",
    "flow": "flow",
    "lint": "eslint --ext .jsx --ext .js src/",
    "quality": "concurrently --names 'TEST,LINT,FLOW' -c 'bgBlue.bold,bgMagenta.bold,black.bgGreen.bold' 'npm run test' 'npm run lint' 'npm run flow'",
    "start": "NODE_ENV=development webpack-dev-server -d --open",
    "start:flow": "nodemon --exec 'npm run flow || true'",
    "start:lint": "nodemon --exec 'npm run lint || true'",
    "start:test": "nodemon --exec 'npm run test || true'",
    "test": "NODE_ENV=test karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apolitical/profile.git"
  },
  "author": "Daniel Mason <daniel.mason@apolitical.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apolitical/profile/issues"
  },
  "homepage": "https://github.com/apolitical/profile#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.4.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "concurrently": "^3.6.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.77.0",
    "html-webpack-plugin": "^3.0.6",
    "karma": "^2.0.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-nyan-reporter": "^0.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.13",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.1",
    "nyc": "^12.0.2",
    "sinon": "^4.4.2",
    "sinon-chai": "^3.2.0",
    "webpack": "^3.0.0",
    "webpack-cdn-plugin": "^1.8.1",
    "webpack-cli": "^1.0.10",
    "webpack-dev-server": "^2.1.0"
  }
}
