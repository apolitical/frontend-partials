{
  "name": "frontend-skeleton",
  "private": true,
  "version": "0.0.0",
  "description": "Skeleton frontend application for Apolitical",
  "main": "index.js",
  "engines": {
    "node": "^10.11.0",
    "npm": "^6.4.1"
  },
  "scripts": {
    "coverage": "codecov -f coverage/lcov.txt",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "dev": "concurrently --names 'SRVR,TEST,LINT,FLOW'  -c 'black.bgCyan.bold,bgBlue.bold,bgMagenta.bold,black.bgGreen.bold' 'npm run start' 'npm run start:test' 'npm run start:lint' 'npm run start:flow'",
    "flow": "flow",
    "lint": "eslint --ext .jsx --ext .js src/",
    "quality": "concurrently --names 'TEST,LINT,FLOW' -c 'bgBlue.bold,bgMagenta.bold,black.bgGreen.bold' 'npm run test' 'npm run lint' 'npm run flow'",
    "start": "NODE_ENV=development webpack-dev-server -d --open",
    "start:flow": "nodemon --exec 'npm run flow || true'",
    "start:lint": "nodemon --exec 'npm run lint || true'",
    "start:test": "nodemon --exec 'npm run test || true'",
    "test": "jest --coverage",
    "audit": "npm-audit-ci -h",
    "preversion": "npm test && npm run lint && npm run flow",
    "version": "npm run build && node version-bump",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apolitical/frontend-skeleton.git"
  },
  "author": "Daniel Mason <daniel.mason@apolitical.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apolitical/frontend-skeleton/issues"
  },
  "homepage": "https://github.com/apolitical/frontend-skeleton#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "core-decorators": "^0.20.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "codecov": "^3.1.0",
    "concurrently": "^4.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flow-header": "^0.2.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "flow-bin": "^0.89.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "npm-audit-ci": "^1.1.0",
    "nyc": "^13.1.0",
    "puppeteer": "^1.11.0",
    "regenerator-runtime": "^0.13.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.3",
    "webpack-cdn-plugin": "^2.3.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
