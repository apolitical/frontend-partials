{
  "name": "profile",
  "private": true,
  "version": "0.0.0",
  "description": "Profile browser frontend application for Apolitical",
  "main": "index.js",
  "engines": {
    "node": "^10.5.0",
    "npm": "^6.1.0"
  },
  "scripts": {
    "coverage": "codecov -f coverage/lcov.txt",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "dev": "concurrently --names 'SRVR,TEST,LINT,FLOW'  -c 'black.bgCyan.bold,bgBlue.bold,bgMagenta.bold,black.bgGreen.bold' 'npm run start' 'npm run start:test' 'npm run start:lint' 'npm run start:flow'",
    "flow": "flow",
    "lint": "eslint --ext .jsx --ext .js src/",
    "quality": "concurrently --names 'TEST,LINT,FLOW' -c 'bgBlue.bold,bgMagenta.bold,black.bgGreen.bold' 'npm run test' 'npm run lint' 'npm run flow'",
    "start": "NODE_ENV=development webpack-dev-server -d --open",
    "start:flow": "nodemon --exec 'npm run flow || true'",
    "start:lint": "nodemon --exec 'npm run lint || true'",
    "start:test": "nodemon --exec 'npm run test || true'",
    "test": "NODE_ENV=test karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apolitical/profile.git"
  },
  "author": "Daniel Mason <daniel.mason@apolitical.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apolitical/profile/issues"
  },
  "homepage": "https://github.com/apolitical/profile#readme",
  "dependencies": {
    "@babel/core": "^7.0.0-rc.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.1",
    "@babel/polyfill": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-flow": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "styled-components": "^3.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-istanbul": "^4.1.6",
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "concurrently": "^3.6.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.79.1",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-nyan-reporter": "^0.2.5",
    "karma-webpack": "^4.0.0-beta.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "puppeteer": "^1.7.0",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.16.5",
    "webpack-cdn-plugin": "^2.2.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
